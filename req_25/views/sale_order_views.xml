<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="custom_sale_order_id" model="ir.ui.view">
        <field name="name">custom sale order</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group//group//field[@name='partner_id']" position="after">
                <field name="total_profit_margin"/>
                <field name="total_profit_value"/>
            </xpath>
        </field>
    </record>

</odoo>

<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="project_tags_id_data" model="ir.ui.view">
        <field name="name">project.tags.view.name</field>
        <field name="model">project.tags</field>
        <field name="inherit_id" ref="project.project_tags_tree_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="users_worked" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>
</odoo>
